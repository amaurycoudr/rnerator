{
  "name": "rnerator",
  "version": "0.2.0",
  "description": "Component generator for React Native, with a Sandbox environment",
  "author": "amaurycoudr @amaurycoudr",
  "bin": {
    "rnerator": "./bin/run"
  },
  "homepage": "https://github.com/amaurycoudr/rnerator",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "amaurycoudr/rnerator",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.0",
    "chalk": "4.1.2",
    "extra": "^0.2.1",
    "fs-extra": "^10.1.0",
    "gradient-string": "^2.0.1",
    "handlebars": "^4.7.7"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.7.6",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "globby": "^11",
    "install-peerdeps": "^3.0.3",
    "mocha": "^9",
    "npx": "^10.2.2",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "rnerator",
    "dirname": "rnerator",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/amaurycoudr/rnerator/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
